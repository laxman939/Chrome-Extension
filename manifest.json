{
    "name": "Bookmark Multiple Websites",
    "description": "Open Multiple Links Easily at a time.",
    "version": "1.0.1",
    
    "manifest_version": 2,
    
    "browser_action": {
        "default_icon":"icon.png",
        "default_popup":"popup.html"
    },
    "permissions":[
        "tabs",
        "storage"
    ],
    "content_security_policy":"script-src 'self' https://apis.google.com; object-src 'self'"
    
}